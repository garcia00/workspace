package br.com.j.hemopa.digital.model;

public enum Horario {
	
	1("	08:00	"),
	2("	08:30	")	,
	3	("	09:00	")	,
	4	("	09:30	")	,
	5	("	10:00	")	,
	6	("	10:30	")	,
	7	("	11:00	")	,
	8	("	11:30	")	,
	9	("	12:00	")	,
	10	("	12:30	")	,
	11	("	13:00	")	,
	12	("	13:30	")	,
	13	("	14:00	")	,
	14	("	14:30	")	,
	15	("	15:00	")	,
	16	("	15:30	")	,
	17	("	16:00	")	,
	18	("	16:30	")	,
	19	("	17:00	")	,
	20	("	17:30	")	,
	21	("	18:00	")	,
	22	("	18:30	")	,

	
	;
	private String descricao;
	
	public void setDescricao(String descricao) {
		this.descricao = descricao;
	}

	Horario (String descricao){
		this.descricao = descricao;
	}
	
	public String getDescricao(){
		return descricao;
	}

}
